    "outputs": [
      "npm:@josfox/jos@4.0.5"
    ]
  },
  "guardrails": {
    "avoid": [
      "git-tag-creation",
      "force-publish"
    ],
    "max_retries": 1,
    "require_clean_git": true
  },
  "intention": {
    "objective": "Publish the @josfox/jos package version 4.0.5 to npm registry",
    "success_criteria": "npm publish returns exit code 0 and package is visible via npm view"
  },
  "capabilities": [
