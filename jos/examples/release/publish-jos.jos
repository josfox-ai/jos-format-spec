{
  "$schema": "https://josfox.ai/schemas/jos-0.0.7.json",
  "$comment": "Format version v0.0.7 ‚Äî Specification maturity v0.1.0 (Alpha)",
  "jos": {
    "open": "jos run atom",
    "supports": [
      "@josfox/jos",
      "josctl"
    ],
    "canonical": "Format version v0.0.7 ‚Äî Specification maturity v0.1.0 (Alpha)"
  },
  "orchestration_contract": {
    "version": "0.0.7",
    "mode": "sync"
  },
  "id_jos": "atom-publish-jos-v1",
  "adaptive_ai": {
    "learning_mode": "frozen",
    "llm_friendly_sentence": "This artifact publishes the @josfox/jos package to npm registry with version 4.0.5"
  },
  "security": {
    "type": "open",
    "permissions": [
      "fs:read",
      "fs:write",
      "net:npm-publish"
    ],
    "health_check": null,
    "integrity": null,
    "integrity_ref": "./publish-jos.jos.sig.json"
  },
  "files": [],
  "orchestration": {
    "state": {
      "current_status": "idle"
    },
    "definitions": {
      "preflight": {
        "type": "shell",
        "description": "Verify package.json exists and npm is authenticated",
        "command": "cd /Users/jos/Documents/dev/jos/jos && echo 'üîç Preflight: Checking @josfox/jos...' && cat package.json | head -5"
      },
      "version_bump": {
        "type": "shell",
        "description": "Bump @josfox/jos to version 4.0.8",
        "command": "cd /Users/jos/Documents/dev/jos/jos && npm version 4.0.8 --no-git-tag-version --allow-same-version && echo '‚úÖ @josfox/jos bumped to 4.0.8'"
      },
      "publish": {
        "type": "shell",
        "description": "Publish @josfox/jos to npm registry (LIVE)",
        "command": "cd /Users/jos/Documents/dev/jos/jos && npm publish --access public"
      },
      "postflight": {
        "type": "shell",
        "description": "Verify publication succeeded",
        "command": "echo '‚úÖ @josfox/jos publish step complete'"
      },
      "guardrails_fix": {
        "type": "shell",
        "description": "Guardrails check - no force-publish",
        "command": "echo 'Guardrails validated'"
      }
    },
    "flows": {
      "main": {
        "description": "Full publish flow with validation",
        "steps": [
          "preflight",
          "version_bump",
          "publish",
          "postflight"
        ]
      },
      "dry_run": {
        "description": "Simulated publish without actual npm publish",
        "steps": [
          "preflight",
          "version_bump",
          "postflight"
        ]
      }
    },
    "pipelines": {
      "publish": {
        "description": "Alias for main flow",
        "steps": [
          "preflight",
          "version_bump",
          "publish",
          "postflight"
        ]
      }
    }
  },
  "x_run_params": {
    "timeout": 300,
    "retry_on_failure": false
  },
  "meta": {
    "version": "1.0.0",
    "type": "atom",
    "name": "publish-jos",
    "provider": "local",
    "author": "JOS Release Engineering",
