{
  "$schema": "https://josfox.ai/schemas/jos-0.0.7.json",
  "jos": {
    "open": "jos run pipeline",
    "supports": ["@josfox/jos", "josctl"]
  },
  "orchestration_contract": {
    "version": "0.0.7",
    "mode": "sync"
  },
  "security": {
    "type": "open",
    "permissions": ["fs:read", "net:fetch"],
    "health_check": null,
    "integrity": null
  },
  "files": [
    "./hello.jos",
    "./timestamp.jos",
    "./sysinfo.jos"
  ],
  "orchestration": {
    "state": { "current_status": "idle" },
    "definitions": {
      "banner": {
        "type": "shell",
        "command": "echo '\\nğŸ¦Š JOS Pipeline Demo\\nâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•'"
      },
      "run_hello": {
        "type": "jos",
        "artifact": "./hello.jos"
      },
      "run_timestamp": {
        "type": "jos",
        "artifact": "./timestamp.jos"
      },
      "run_sysinfo": {
        "type": "jos",
        "artifact": "./sysinfo.jos"
      },
      "summary": {
        "type": "shell",
        "command": "echo '\\nâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\nâœ… Pipeline completed successfully!\\n'"
      }
    },
    "flows": {
      "main": {
        "steps": ["banner", "run_hello", "run_timestamp", "run_sysinfo", "summary"]
      }
    },
    "pipelines": {
      "demo": {
        "description": "Full demo pipeline running all atoms",
        "steps": ["banner", "run_hello", "run_timestamp", "run_sysinfo", "summary"]
      }
    }
  },
  "meta": {
    "version": "1.0.0",
    "type": "pipeline",
    "name": "demo-pipeline",
    "provider": "local",
    "author": "JOS Examples"
  },
  "artifacts": {
    "description": "Orchestrator that runs hello, timestamp, and sysinfo atoms in sequence"
  },
  "guardrails": {
    "avoid": [],
    "max_retries": 1
  },
  "intention": {
    "objective": "Demonstrate fractal composition by orchestrating multiple .jos atoms",
    "success_criteria": "All three sub-artifacts execute successfully in sequence"
  },
  "capabilities": ["orchestration", "pipeline", "demo"]
}
